
config mosdns 'config'
	option geo_update_day_time '2'
	option redirect '1'
	option autoconf '1'
	option syncconfig '1'
	option loglv 'error'
	option not_first_start '1'
	option geo_auto_update '1'
	option geo_update_week_time '*'
	option configfile '/etc/mosdns/config_custom.yaml'
	option enabled '1'

